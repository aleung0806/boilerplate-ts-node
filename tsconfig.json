{
  "compilerOptions":{
    "noImplicitAny": false,
    "outDir": "./build",
    "allowJs": true,
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": false, //change
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "baseUrl": "./",
  },
  "include": [
  "./src/**/*"
],
"typeRoots": [
  "./node_modules/@types",
  "./types",
], 
}